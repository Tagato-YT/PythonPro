import random
Password = ""
caracteres_python = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789!\"#$%&'()*+,-./:;<=>?@[\\]^_`{|}~"
Cantidad = int(input("Cuantos caracteres quieres que tenga tu Contraseña: "))
for i in range (Cantidad):
    Password += random.choice(caracteres_python)
print("Tu contraseña es: ", Password)
